<div class="custom-card">
  <div class="custom-card__image">
    <img
      src="{{ image_url | image_url }}"
      alt="{{ image_alt }}"
      width="{{ image.width }}"
      height="{{ image.height }}"
    >
  </div>

  <div class="custom-card__content">
    <div class="custom-card__title">{{ title }}</div>
  </div>
</div>
